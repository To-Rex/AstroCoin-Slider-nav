<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:elevation="3dp"
        android:layout_width="350dp"
        android:background="#00FFFFFF"
        android:layout_height="350dp">
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/ic_card__1_">
        <ImageView
            android:id="@+id/setshapimguser01"
            android:layout_width="340dp"
            android:layout_height="300dp"
            android:scaleType="centerCrop"
            android:src="@drawable/ic_profile_circle"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="4dp"
            tools:ignore="ContentDescription" />

        <TextView
                android:text="@string/connect"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView901"
                app:layout_constraintStart_toStartOf="@+id/setshapimguser01"
                android:layout_marginStart="20dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/setshapimguser01"/>
    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>
